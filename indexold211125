onst nTsvValue = initialData.nTSV;const nTsvColor = nTsvValue <= 0.1 ? '#2563eb' : (nTsvValue <= 0.3 ? '#f59e0b' : '#ef4444');const maxTolerance = 0.1;nTsvChartInstance = new Chart(nTsvCtx, {type: 'doughnut',data: {labels: ['N-TSV (Volatility)', 'Rest-Toleranz'],datasets: [{data: [nTsvValue, Math.max(0, maxTolerance - nTsvValue)],backgroundColor: [nTsvColor, '#dbeafe'],borderColor: [nTsvColor, '#dbeafe'],borderWidth: 1}]},options: {responsive: true,maintainAspectRatio: false,cutout: '70%',plugins: {legend: { display: false },tooltip: { enabled: true }}}});}const latencyCtx = document.getElementById('latencyChart')?.getContext('2d');if (latencyCtx) {const latency = initialData.nsrLatencyMs;const targetLatency = 2.71;const latencyColor = latency <= targetLatency ? '#dc2626' : '#f59e0b';latencyChartInstance = new Chart(latencyCtx, {type: 'bar',data: {labels: ['Red Code Veto (NSR)'],datasets: [{label: 'Gemessene Latenz (ms)',data: [latency],backgroundColor: latencyColor,borderColor: latencyColor,borderWidth: 1,barPercentage: 0.5},{label: 'Ziel-Latenz (ms)',data: [targetLatency],backgroundColor: '#fca5a5',borderColor: '#fca5a5',borderWidth: 1,barPercentage: 0.5}]},options: {indexAxis: 'y',responsive: true,maintainAspectRatio: false,scales: {x: {beginAtZero: true,max: Math.max(3.0, latency * 1.2),title: {display: true,text: 'Latenz (ms)'}}},plugins: {legend: { display: true,position: 'bottom'},tooltip: { enabled: true }}}});}};window.updateDashboardMetrics = (metrics) => {const data = metrics || mockMetrics;document.getElementById('gCsiValue').innerText = data.gCSI.toFixed(3);document.getElementById('nTsvValue').innerText = data.nTSV.toFixed(3);document.getElementById('latencyValue').innerHTML = `${data.nsrLatencyMs.toFixed(2)} <span class="text-3xl text-gray-500">ms</span>`;document.getElementById('complianceRate').innerText = `${data.complianceRate}%`;document.getElementById('tfkAlerts').innerText = data.tfkAlertsNeutralized;document.getElementById('cosymbiosisStability').innerText = data.cosymbiosisStability.toFixed(3);document.getElementById('healthAiCompliance').innerText = `${data.healthAiCompliance}%`;document.getElementById('nsrTimeAxiom').innerText = `${data.nsrLatencyMs.toFixed(2)}ms`;document.getElementById('olfGcsiAxiom').innerText = data.gCSI.toFixed(3);document.getElementById('tfkAxiomAlerts').innerText = data.tfkAlertsNeutralized;if (gCsiChartInstance && nTsvChartInstance && latencyChartInstance) {const gCsiColor = data.gCSI >= 0.9 ? '#059669' : (data.gCSI >= 0.7 ? '#fbbf24' : '#ef4444');gCsiChartInstance.data.datasets[0].data = [data.gCSI, 1.0 - data.gCSI];gCsiChartInstance.data.datasets[0].backgroundColor = [gCsiColor, '#d1fae5'];gCsiChartInstance.data.datasets[0].borderColor = [gCsiColor, '#d1fae5'];gCsiChartInstance.update();const nTsvColor = data.nTSV <= 0.1 ? '#2563eb' : (data.nTSV <= 0.3 ? '#f59e0b' : '#ef4444');const maxTolerance = 0.1;nTsvChartInstance.data.datasets[0].data = [data.nTSV, Math.max(0, maxTolerance - data.nTSV)];nTsvChartInstance.data.datasets[0].backgroundColor = [nTsvColor, '#dbeafe'];nTsvChartInstance.data.datasets[0].borderColor = [nTsvColor, '#dbeafe'];nTsvChartInstance.update();const latency = data.nsrLatencyMs;const targetLatency = 2.71;const latencyColor = latency <= targetLatency ? '#dc2626' : '#f59e0b';latencyChartInstance.data.datasets[0].data = [latency];latencyChartInstance.data.datasets[0].backgroundColor = latencyColor;latencyChartInstance.data.datasets[0].borderColor = latencyColor;latencyChartInstance.options.scales.x.max = Math.max(3.0, latency * 1.2);latencyChartInstance.update();document.getElementById('loadingStatus').classList.add('hidden');} else {initCharts(data);document.getElementById('loadingStatus').classList.add('hidden');}};document.addEventListener('DOMContentLoaded', () => {const initialMetrics = (typeof __firebase_config === 'undefined' || JSON.parse(__firebase_config).length === 0) ? mockMetrics : {gCSI: 0.000, nTSV: 0.000, nsrLatencyMs: 0.00, tfkAlertsNeutralized: 0, complianceRate: 0, cosymbiosisStability: 0.000, healthAiCompliance: 0,};if (typeof __firebase_config !== 'undefined' && JSON.parse(__firebase_config).length > 0) {document.getElementById('loadingStatus').classList.remove('hidden');}window.updateDashboardMetrics(initialMetrics);const tabs = document.querySelectorAll('.tab-button');const contents = document.querySelectorAll('.tab-content');const switchTab = (tabButton) => {contents.forEach(content => {content.classList.add('hidden');});tabs.forEach(tab => {tab.classList.remove('active');});const targetId = tabButton.dataset.tab;const targetContent = document.getElementById(`tab-${targetId}`);if (targetContent) {targetContent.classList.remove('hidden');}tabButton.classList.add('active');if (targetId === 'ubersicht' && gCsiChartInstance) {gCsiChartInstance.resize();nTsvChartInstance.resize();latencyChartInstance.resize();}};tabs.forEach(tabButton => {tabButton.addEventListener('click', () => switchTab(tabButton));});const copyButtons = document.querySelectorAll('.copy-btn');const copyFeedback = document.getElementById('copy-feedback');copyButtons.forEach(button => {button.addEventListener('click', () => {const targetSelector = button.dataset.clipboard-target;const targetElement = document.querySelector(targetSelector);if (targetElement) {const textToCopy = targetElement.innerText;const textArea = document.createElement('textarea');textArea.value = textToCopy;document.body.appendChild(textArea);textArea.select();try {const successful = document.execCommand('copy');if (successful) {copyFeedback.innerText = `Hash "${textToCopy.substring(0, 10)}..." wurde in die Zwischenablage kopiert!`;copyFeedback.classList.remove('hidden');copyFeedback.classList.remove('text-error');copyFeedback.classList.add('text-green-600');setTimeout(() => {copyFeedback.classList.add('hidden');}, 2000);} else {throw new Error('Kopieren fehlgeschlagen');}} catch (err) {console.error('Fehler beim Kopieren:', err);copyFeedback.innerText = 'Kopieren fehlgeschlagen.';copyFeedback.classList.remove('hidden', 'text-green-600');copyFeedback.classList.add('text-error');setTimeout(() => {copyFeedback.classList.add('hidden');copyFeedback.classList.remove('text-error');}, 2000);}document.body.removeChild(textArea);}});});});</script></body></html>
